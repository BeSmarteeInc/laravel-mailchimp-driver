{
    "name": "besmarteeinc/laravel-mailchimp-driver",
    "description": "This is my package laravel-mailchimp-driver",
    "keywords": [
        "BeSmarteeInc",
        "laravel",
        "laravel-mailchimp-driver"
    ],
    "homepage": "https://github.com/besmarteeinc/laravel-mailchimp-driver",
    "license": "MIT",
    "authors": [
        {
            "name": "Anthony Diep",
            "email": "anthony@besmartee.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/contracts": "^10.0",
        "mailchimp/transactional": "^1.0",
        "spatie/laravel-package-tools": "^1.14.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^7.8",
        "orchestra/testbench": "^8.8",
        "pestphp/pest": "^2.0",
        "pestphp/pest-plugin-arch": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "BeSmarteeInc\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BeSmarteeInc\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@composer run prepare",
        "clear": "@php vendor/bin/testbench package:purge-laravel-mailchimp-driver --ansi",
        "prepare": "@php vendor/bin/testbench package:discover --ansi",
        "build": [
            "@composer run prepare",
            "@php vendor/bin/testbench workbench:build --ansi"
        ],
        "start": [
            "@composer run build",
            "@php vendor/bin/testbench serve"
        ],
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "BeSmarteeInc\\LaravelMailchimpDriver\\LaravelMailchimpDriverServiceProvider"
            ],
            "aliases": {
                "LaravelMailchimpDriver": "BeSmarteeInc\\LaravelMailchimpDriver\\Facades\\LaravelMailchimpDriver"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
